<html>
	<head>
		<title>Bill Of Lading</title>
		<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">

		<script type="text/javascript" src="/static/jquery/jquery-1.10.2.min.js"></script>
		<script type="text/javascript" src="/static/js/customer.js"></script>
		<script type="text/javascript" src="/static/js/deployJava.js"></script>
		<script type="text/javascript" src="/static/js/applet-label-print.js"></script>
		<script type="text/javascript" src="/static/js/html2canvas/html2canvas.js"></script>
		<script type="text/javascript" src="/static/js/html2canvas/jquery.plugin.html2canvas.js"></script>
	</head>

	<body bgcolor="#FFFFFF" text="#000000" leftmargin="0" topmargin="0" marginwidth="0" marginheight="0">
		<div id="print-content">
			<table width="660" border="0" height="99" cellpadding="0" cellspacing="0">
				<tr>
				<td width="220" height="86" valign="top"><img id="logo" src="/static/branding/[% branding_id %]/images/header/report-logo.png" valign=top height="[% bol_logo_height %]">[% bol_url_phone %]
				<td width="220" height="86" valign=top>
					<div align="center"> <font face="Arial, Helvetica, sans-serif" size="3">
					<b><font size="2">PRO NUMBER</font></b></font><br>
					<font face="Arial, Helvetica, sans-serif" size="2">[% tracking1 %]</font><br>
						<img id="barcode" src="[% barcode_image %]" alt="PLACE PRO LABEL HERE" width="200" height="30">
					</b></font> </div>
				</td>
				<td width="220" height="86" valign="top">
					<div align="right"><img id="bol" src="[% bol_image %]" width="200" height="69"></div>
					<div align="left"><font color="#000000" size="2" face="Arial, Helvetica, sans-serif"><b><font size="1">Carrier of Record: &nbsp;[% carrier %]</div>
					</font></b></font>
				</td>
				</tr>
			</table>
			<table width="661" height="130" cellpadding="0" cellspacing="0" border="1" bordercolor="#000000">
				<tr>
				 <td height="11" width="141" bgcolor="#000000" bordercolor="#000000">
					<b>&nbsp;<font face="Arial, Helvetica, sans-serif" size="1" color="#FFFFFF">Order Number:&nbsp;
					[% ordernumber %]</font></b></td>
				<td height="11" width="120" bgcolor="#000000" bordercolor="#000000">
					<b>&nbsp;<font face="Arial, Helvetica, sans-serif" size="1" color="#FFFFFF">Date:
				  &nbsp;[% dateshipped %]</font></b></td>
				<td height="11" width="200" bgcolor="#000000" bordercolor="#000000">
					<font face="Arial, Helvetica, sans-serif" size="1" color="#FFFFFF">&nbsp;
						<b>P.O. Number: &nbsp;[% ponumber %]</b></font>
				</td>
				 <td height="11" width="250" bgcolor="#000000" bordercolor="#000000">
					<b>&nbsp;<font face="Arial, Helvetica, sans-serif" size="1" color="#FFFFFF">
					Customer Reference: [% custnum %]&nbsp;
					</font></b></td>
				</tr>
				<tr>
				<td height="8" colspan="2" bgcolor="#000000" bordercolor="#000000">
					<div align="center"><font size="1" face="Arial, Helvetica, sans-serif" color="#FFFFFF">
						<b>Consignee&nbsp;&nbsp;(TO)</b></font></div>
				</td>
				<td height="8" colspan="2" bgcolor="#000000" bordercolor="#000000">
					<div align="center"><font size="1" face="Arial, Helvetica, sans-serif" color="#FFFFFF">
						<b>Consignor&nbsp;&nbsp;(FROM)</b></font></div>
				</td>
				</tr>
				<tr>
				<td height="11" colspan="2">
					<div align="center"><font face="Arial, Helvetica, sans-serif" size="1">
						[% addressname %]&nbsp;</font></div>
				</td>
				<td height="11" colspan="2">
					<div align="center"><font size="1" face="Arial, Helvetica, sans-serif">
						[% shipasname %]&nbsp;</font></div>
				</td>
				</tr>
				<tr>
				<td height="8" colspan="2">
					<div align="center"><font face="Arial, Helvetica, sans-serif" size="1">
						[% address1 %]</font></div>
				</td>
				<td height="8" colspan="2">
					<div align="center"><font size="1" face="Arial, Helvetica, sans-serif">
						[% branchaddress1 %]&nbsp;</font></div>
				</td>
				</tr>
				<tr>
				<td height="8" colspan="2"> <div align="center"><font face="Arial, Helvetica, sans-serif" size="1">
						[% address2 %]&nbsp;</font></div></td>
				<td height="8" colspan="2"> <div align="center"><font face="Arial, Helvetica, sans-serif" size="1">
						[% branchaddress2 %]&nbsp;</font></div></td>
				</tr>
				<tr>
				<td height="4" colspan="2">
					<div align="center"><font size="1" face="Arial, Helvetica, sans-serif">
						[% addresscity %], [% addressstate %] [% addresszip %]&nbsp;</font></div>
				</td>
				<td height="4" colspan="2"> <div align="center"><font face="Arial, Helvetica, sans-serif" size="1">
						[% branchaddresscity %], [% branchaddressstate %] [% branchaddresszip %]&nbsp;</td></font></div>
				</tr>
				<tr>
				<td height="12" colspan="2"><div align="center"><font face="Arial, Helvetica, sans-serif" size="1">
						[% addresscountry %]&nbsp;</td></font></div>
				<td height="12" colspan="2"><div align="center"><font face="Arial, Helvetica, sans-serif" size="1">
						[% branchaddresscountry %]&nbsp;</td></font></div>
				</tr>
				<tr>
				<td height="2" colspan="2"><div align="center"><font face="Arial, Helvetica, sans-serif" size="1">
						[% contactname %]&nbsp;[% contactphone %]&nbsp;</td></font></div>
				<td height="2" colspan="2"><div align="center"><font face="Arial, Helvetica, sans-serif" size="1">
						[% branchcontact %]&nbsp;[% branchphone %]&nbsp;</td></font></div>
				</tr>
				<tr>
				<td height="2" colspan="2"><font face="Arial, Helvetica, sans-serif" size="1">&nbsp;Emergency
			  Response No.</font></td>
				<td height="2" colspan="2">&nbsp;<font face="Arial, Helvetica, sans-serif" size="1">Route</font></td>
				</tr>
				</table>
				<table width="661" cellpadding="0" cellspacing="0" border="0">
				<tr>
				<td><div align=center><font size="1" face="Arial, Helvetica, sans-serif">SUBJECT TO CORRECTION</font></div>
				</td>
				</tr>
				</table>
				<table cellpadding="0" cellspacing="0" border="1" bordercolor="#000000" height="21" width="662" bgcolor="#FFFFFF">
				<tr bgcolor="#999999" bordercolor="#999999">
				<td height="17" width="64" bordercolor="#000000" bgcolor="#000000">
				<div align="center"><font face="Arial, Helvetica, sans-serif" size="-3" color="#FFFFFF">Number
			  Pack.</font></div>
				</td>
				<td height="17" width="29" bordercolor="#000000" bgcolor="#000000">
				<div align="center"><font size="-2" face="Arial, Helvetica, sans-serif" color="#FFFFFF">HM</font></div>
				</td>
				<td height="17" width="51" bordercolor="#000000" bgcolor="#000000">
				<div align="center"><font size="-2" face="Arial, Helvetica, sans-serif" color="#FFFFFF">Packing
			  Type</font></div>
				</td>
				<td height="17" width="239" bordercolor="#000000" bgcolor="#000000">
				<div align="center"><font size="-2" face="Arial, Helvetica, sans-serif" color="#FFFFFF">Description
			  of Articles,Special Marks &amp; Exceptions</font></div>
				</td>
				<td height="17" width="61" bordercolor="#000000" bgcolor="#000000">
				<div align="center"><font size="-2" face="Arial, Helvetica, sans-serif" color="#FFFFFF">DIMS
			  LxWxH</font></div>
				</td>
				<td height="17" width="48" bordercolor="#000000" bgcolor="#000000">
				<div align="center"><font size="-2" face="Arial, Helvetica, sans-serif" color="#FFFFFF">WEIGHT</font></div>
				</td>
				<td height="17" width="85" bordercolor="#000000" bgcolor="#000000">
				<div align="center"><font size="-2" face="Arial, Helvetica, sans-serif" color="#FFFFFF">Class
			  or Rate Ref</font></div>
				</td>
				<td height="17" width="67" bordercolor="#000000" bgcolor="#000000">
				<div align="center"><font size="-2" face="Arial, Helvetica, sans-serif" color="#FFFFFF">CUBE
			  (Optional)</font></div>
				</td>
				</tr>
			<!-- Product Entries-->

				[% FOREACH item IN bol_packagelist_loop %]
				<tr>
					<td width="8%" align="[% item.datatypeid == 2000 ? 'right' : 'center' %]">
						<font size="1" face="Arial, Helvetica, sans-serif">
							&nbsp;[% item.quantity %]&nbsp;
						</font>
					</td>
					<td width="6%" align="center">
						<font face="Arial, Helvetica, sans-serif" size="1">
							<input type="checkbox" name="hazardous" value="hazardous" [% item.hazardcheck %]>
						</font>
					</td>
					<td width="9%">
						<font size="1" face="Arial, Helvetica, sans-serif">
							&nbsp;[% item.unittype %]
						</font>
					</td>
					<td width="47%">
						<font face="Arial, Helvetica, sans-serif" size="1">
							&nbsp;[% item.description %]
						</font>
					</td>
					<td width="5%" align=center>
						<font face="Arial, Helvetica, sans-serif" size="1">
							&nbsp;[% item.dims %]
						</font>
					</td>
					<td width="6%" align="[% item.datatypeid == 2000 ? 'right' : 'center' %]">
						<font face="Arial, Helvetica, sans-serif" size="1">
							&nbsp;[% item.weight %]&nbsp;
						</font>
					</td>
					<td width="10%" align=center>
						<font face="Arial, Helvetica, sans-serif" size="1">
							&nbsp;[% item.freightclass %]&nbsp;
						</font>
					</td>
					<td width="9%">
						&nbsp;
					</td>
				</tr>
				[% END %]

			<!-- Product Entries-->

				<table width="661" cellpadding="0" cellspacing="0" border="0">
				<tr>
				<td width="8%" align="center">
					<font size="1" face="Arial, Helvetica, sans-serif">
						&nbsp;[% packagetotalquantity %]&nbsp;
					</font>
				</td>
				<td width="6%" align="center">
					<font face="Arial, Helvetica, sans-serif" size="1">
						<input type="checkbox" name="packagetotalhazardous" value="hazardous" [% hazardcheck %]>
					</font>
				</td>
				<td width="9%">
					<font size="1" face="Arial, Helvetica, sans-serif">
						&nbsp;[% packagetotalunittype %]
					</font>
				</td>
				<td width="47%">
					<font face="Arial, Helvetica, sans-serif" size="1">
						&nbsp;[% packagetotaldescription %]
					</font>
				</td>
				<td width="5%" align=center>
					<font face="Arial, Helvetica, sans-serif" size="1">
						&nbsp;[% packagetotaldims %]
					</font>
				</td>
				<td width="6%" align=center><div align=center>
					<font face="Arial, Helvetica, sans-serif" size="1">
						&nbsp;[% packagetotalweight %]&nbsp;
					</font></div>
				</td>
				<td width="10%" align=center>
					<font face="Arial, Helvetica, sans-serif" size="1">
						&nbsp;[% packagetotalfreightclass %]&nbsp;
					</font>
				</td>
				<td width="9%">
					&nbsp;
				</td>
				</tr>
				</table>
				</table>
				<table width="661" cellpadding="0" cellspacing="0" border="1" bordercolor="#000000">
				<tr>
					<td width="214" bordercolor="#FFFFFF">
						<font face="Arial, Helvetica, sans-serif" size="2"><i>Bill To:</i></font>
					</td>
					<td width="142" bordercolor="#FFFFFF">&nbsp;</td>
					<td width="129" bordercolor="#FFFFFF" align=left>
						<font size="1" face="Arial, Helvetica, sans-serif">&nbsp;&nbsp;Freight Charges:</font>
					</td>
					<td width="165" bordercolor="#FFFFFF" colspan=3>&nbsp;</td>
				</tr>
				<tr>
					<td width="214" bordercolor="#FFFFFF" align=left>
						<font face="Arial, Helvetica, sans-serif" size="2"><b> [% billingname %]</b></font>
					</td>
					<td width="142" bordercolor="#FFFFFF">
						<font face="Arial, Helvetica, sans-serif" size="1">COD AMT:&nbsp;
						[% IF codamount %]
							&nbsp;$[% codamount %]
						[% ELSE %]
							&nbsp;$__________
						[% END %]
						</font>
					</td>
					<td width="129" bordercolor="#FFFFFF" align=left>
						<font face="Arial, Helvetica, sans-serif" size="1">&nbsp;
							<input type=radio name=freightcharges value=0 [% freightcharges == 0 ? 'CHECKED' : '' %]> Prepaid<br>
						</font>
					</td>
					<td width="165" bordercolor="#FFFFFF" colspan=3>&nbsp;</td>
				</tr>
				<tr>
					<td width="214" bordercolor="#FFFFFF">
						<font size="2" face="Arial, Helvetica, sans-serif"><b>[% billingaddress1 %]</b></font>
					</td>
					<td width="142" bordercolor="#FFFFFF">
						<font face="Arial, Helvetica, sans-serif" size="1">COD FEE:</font>
					</td>
					<td width="129" bordercolor="#FFFFFF" align=left>
						<font face="Arial, Helvetica, sans-serif"  size="1">&nbsp;
							<input type=radio name=freightcharges value=1 [% freightcharges == 1 ? 'CHECKED' : '' %]> Collect<br>
						<br>
						</font>
					</td>
					<td width="165" bordercolor="#FFFFFF" colspan=3>&nbsp;</td>
				</tr>
				<tr>
					<td width="214" bgcolor="#FFFFFF" bordercolor="#FFFFFF" align=left>
						<font face="Arial, Helvetica, sans-serif" size="2"><b>[% billingaddress2 %]</b></font>
					</td>
					<td width="142" bordercolor="#FFFFFF">
						<font face="Arial, Helvetica, sans-serif" size="1">
						[% IF codpaymentmethod %]
						<input type=radio name=codpaymentmethod value='codprepaid' [% codpaymentmethod == 'codprepaid' ? 'CHECKED' : '' %]/> Prepaid
						<input type=radio name=codpaymethod value='codcollect' [% codpaymentmethod == 'codcollect' ? 'CHECKED' : '' %]> Collect
						[% END %]
						</font>
					</td>
					<td width="129" bordercolor="#FFFFFF">
						<font size="1"> <font face="Arial, Helvetica, sans-serif">&nbsp;
						<input type=radio name=freightcharges value=2 [% freightcharges == 2 ? 'CHECKED' : '' %] 3rd Party");
						&nbsp;&nbsp;
						<input type=radio name=freightsingle value='freightsingle' CHECKED> Single Shipment
						</font>
					</td>
					<td width="165" bordercolor="#FFFFFF" colspan=3>&nbsp;</td>
				</tr>
				<tr>
					<td width="214" bordercolor="#FFFFFF" align=left>
						<font size="2" face="Arial, Helvetica, sans-serif"><b>[% billingcity %], [% billingstate %] [% billingzip %]</b></font>
					</td>
					<td colspan=5 bordercolor="#FFFFFF" align=left>
						<font size="1" color="#000000" face="Arial, Helvetica, sans-serif">Is Customer's check acceptable for COD?
							<input type=radio name=codcheck value='Yes' [% codcheck == 'Yes' ? 'CHECKED' : '' %]> Yes
							<input type=radio name=codcheck value='No'  [% codcheck == 'No'  ? 'CHECKED' : '' %]> No
						</font>
					</td>
					 </font>
					</td>
				</tr>
				</table>
				<table width="662">
				<tr>
					<td align=center>
						<font face="Arial, Helvetica, sans-serif" size="1"><b>ARTICLES SUBJECT TO RELEASED VALUE</b></font>
					</td>
				<tr>
					<td align=left>
						<font size="-2" face="Times New Roman, Times, serif">
							NOTE(1) When the rate is dependent on value, shippers are required to state specificially in writing the agreed or
							declared value of the property.  The agreed or declared value of the property is hereby specifically stated by the
							shipper to be not exceeding [% declaredvalue %].
							NOTE (2) LIABILITY LIMITATION FOR LOSS OR DAMAGE ON THIS SHIPMENT MAY BE APPLICABLE. SEE 49 U.S.C 14706 (C)(1)(A)&(B).
							**If no value is shown above, shipment is subject to the lowest actual or released value provided in NMF 100
							Series or other pricing agreements effective between Shipper &amp; carrier; shipments of household goods, personal
							effects or used articles are released to a $.10 per pound piece; shipments originating in Canada are released to a
							value of Can $2.00 per pound. Carriers maximum liability for loss or damage cargo is $25.00 per pound per piece,
							subject to a maximum liability of $100,000.00 per shipment, unless the Shipper/Consignee requests Excess Declared
							Value coverage as provided below. NOTE (3) Commodities requiring special or additional care or attention in handling
							or stowing must be marked and packaged as to ensure safe transportation with ordinary care. See Sec. 2(e) of NMFC item
							360.
						</font>
					</td>
				</tr>
				</table>
				<table width="661" border="1"  cellpadding="0" cellspacing="0" bordercolor="#000000">
				<tr bordercolor="#FFFFFF">
					<td width="661" bordercolor="#FFFFFF">
						<font face="Arial, Helvetica, sans-serif" size=1>
							<b>Special Services Requested</b> (Subject to Carriers additional charges or other pricing agreements)
						</font>
					</td>
				</tr>
				<tr height=67 valign=top>
					<td width="661" bordercolor="#FFFFFF" id="dynamicassessorials">
						<table border="0" cellSpacing="0" cellPadding="0">
							[% count=0 %]
							<tr>
								[% FOREACH item IN assessorial_loop %]
								[% count=count+1 %]
									<td id="td_[% item.value %]">
										<span id="blah"><font size="1" face="Arial, Helvetica, sans-serif"><input type="checkbox" id="[% item.name %]" name="[% item.name %]" value="1" [% item.checked ? 'CHECKED' : '' %]/>&nbsp;[% item.displayname %]&nbsp;&nbsp;</font></span>
									</td>
									[% IF count == 5 %]
									</tr><tr>
										[% count = 1 %]
									[% END %]
								[% END %]
							</tr>
						</table>
					</td>
				</tr>
				<tr>
					<td width="661" bordercolor="#FFFFFF">
						<input type=checkbox name=specialdeclared [% declaredvalue && declaredvalue > 0 ? 'CHECKED' : '' %]>
						<font size="1" face="Arial, Helvetica, sans-serif">
							Shipper/Consignor hereby requests Excess Delared value Coverage based on a declared value of $[% declaredvalue %] .
						</font>
					</td>
				</tr>
				</table>
				<table width="661" border="0" height="170" cellpadding="0" cellspacing="0" bordercolor="#000000">
				<tr>
				<td colspan=8 height="158" bordercolor="#666666"><font size="-2" face="Times New Roman, Times, serif">THIS
			  IS TO CERTIFY THAT THE ABOVE-NAMED MATERIALS ARE PROPERLY CLASSIFIED, PACKAGED,
			  MARKED &amp; LABELED &amp; ARE IN PROPER CONDITION FOR TRANSPORTATION ACCORDING
			  TO THE APPLICABLE REGULATIONS OF THE DEPARTMENT OF TRANSPORTATION. RECEIVED,
			  subject to individually determined rated or contracts that have been agreed
			  upon in writing between the carrier and the shipper, if applicable, otherwise
			  to the rates, classifications and rules that have been established by the
			  carrier and are available to the shipper, on request, the properly described
			  above in apparent good order, except as noted (contents &amp; conditions
									of contents of packages unknown), marked, consigned &amp; destined as indicated
			  above, which said carrier agrees to carry destination, if on route, otherwise
			  or deliver to another carrier on the route to said destination. It is mutually
			  agreed as to each carrier of all or any said property, over all or any portion
			  of said route to destination and as to each party at any time interested
			  in all or any of said property, that every service to be performed hereunder
				  shall be subject to all the terms and conditions of the Uniform Bill of
				  Lading set forth in the National Motor Freight Classification 100-X and
				  successive issues. The shipper certifies that he/she is familiar with all
				  the terms &amp; conditions of the said bill of lading, including those on
				  the back thereof &amp; the said terms &amp; conditions are hereby agreed
				  to by the shipper &amp; accepted for himself / herself &amp; his / her assigns.
				  For Freight Collect Shipments. If this shipment is to be delivered to the
				  consignee without recourse on the consignor, the consignor shall sign the
				  following statement: The carrier may decline to make delivery of this shipment
				  without payment of freight &amp; all other lawful charges. Engage Technology
				  is a third party technology provider and is not responsible for claims. </font><font size="-2" face="Arial, Helvetica, sans-serif"><br>
					<br><div align=right>
				  ____________________________________________ <br>
					<b>Signature of Consignee</b></div>
					</font></td>
				</tr>
			</table>
			<table cellspacing="0" cellpadding="0" align="left" width="661" border="1" bordercolor="#000000">
				<tr>
				<td width=50%> <font face="Arial, Helvetica, sans-serif" size="1"> &nbsp;Shipper/Consignee
					</font> </td>
				<td width=50%> <font face="Arial, Helvetica, sans-serif" size="1"> &nbsp;Carrier
					</font> </td>
				</tr>
				<tr height=30>
				<td width=50% valign=top> <font face="Arial, Helvetica, sans-serif" size="1">
				  &nbsp;Authorized Signature&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Date
					</font> </td>
				<td width=50% valign=top> <font face="Arial, Helvetica, sans-serif" size="1">
				  &nbsp;Authorized Signature&nbsp&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Date
					</font> </td>
				</tr>
			</table>

			<p align="left"><br><br>
			</p>
			<table width="661" border="1" cellspacing="0" cellpadding="0" bordercolor="#333333">
				<tr>
				<td><font size="1" face="Arial, Helvetica, sans-serif">Comments: &nbsp; [% description %]</font></td>
				</tr>
			</table>
			<table width="661" border="0" cellspacing="0" cellpadding="0" bordercolor="#FFFFFF">
				<tr>
				<td><font face="Times New Roman, Times, serif" size="1">Total
			  # of Pieces received ______ Tendered as ______ pallets&nbsp;
				<input type="checkbox" name="saidtocontain">
			  Said to contain
				<input type="checkbox" name="contains">
			  Contains ______ Pieces/Cartons &amp; ______ Loose pieces/Cartons</font></td>
				</tr>
			</table>
			<p align="left"></p>
		</div>

		<form id="frm_next" action="/customer/order/ajax" method="post">
			<input type="hidden" name="ajax" value="1"/>
			<input type="hidden" name="type" value="HTML"/>
			[% IF printcominv %]<input type="hidden" name="action" value="generate_commercial_invoice"/>[% END %]

			<input type="hidden" id="coid" name="coid" value="[% coid %]"/>
			<input type="hidden" id="shipmentid" name="shipmentid" value="[% shipmentid %]"/>
		</form>
		<canvas id="hidden_screenshot" style="display:none;"></canvas>
	</body>

	<script>
	$(document).ready(function() {

		useDefaultPrinter(function() {

			if (!isLoaded())
				{
				alert("Printer is not loaded, please try again.");
				return;
				}

			$("#logo").attr("src",getPath()+$("#logo").attr("src"));
			$("#barcode").attr("src",getPath()+$("#barcode").attr("src"));
			$("#bol").attr("src",getPath()+$("#bol").attr("src"));

			window['qzDonePrinting'] = function() {
				if ($('input[name=action]').length > 0)
					document.getElementById('frm_next').submit();
				else
					{
					var query_param = 'coid=[% coid %]&shipmentid=[% shipmentid %]';
					send_ajax_request('', 'JSON', 'order', 'mark_shipment_as_printed', query_param, function() {
						var href = '/customer/order/quickship';
						if (JSON_data.RETURN_SHIPMENT == 1) href += '?coid=' + JSON_data.RET_COID;
						window.location.href = href;
						});
					}

				// Remove reference to this function
				window['qzDonePrinting'] = null;
				};
			printHTML5Page();
			});
		});
	</script>
</html>

<h2>SHIPMENT-SUMMARY</h2>

<div id="equal">
	<div id="row">
		<div class="add-summary">
			<table class="summary-app">
			<caption>ADDRESS</caption>
			<tr>
				<th>SHIP FROM </th><td>[% shipmentinfo.oacontactname %]</td>
				<th>SHIP TO </th><td>[% shipmentinfo.contactname %]</td>
			</tr>
			<tr>
				<td>&nbsp;</td><td>[% customerAddress.addressname %]</td>
				<td>&nbsp;</td><td>[% toAddress.addressname %]</td>
			</tr>
			<tr>
				<td>&nbsp;</td><td>[% customerAddress.address1 %]</td>
				<td>&nbsp;</td><td>[% toAddress.address1 %]</td>
			</tr>
			[% IF customerAddress.address2 != '' && toAddress.address2 != '' %]
			<tr>
				<td>&nbsp;</td><td>[% customerAddress.address2 %]</td>
				<td>&nbsp;</td><td>[% toAddress.address2 %]</td>
			</tr>
			[% END %]
			<tr>
				<td>&nbsp;</td><td>[% customerAddress.city %], [% customerAddress.state %], [% customerAddress.zip %]</td>
				<td>&nbsp;</td><td>[% toAddress.city %], [% toAddress.state %], [% toAddress.zip %]</td>
			</tr>
			<tr>
				<td>&nbsp;</td><td>[% customerAddress.country_description %]</td>
				<td>&nbsp;</td><td>[% toAddress.country_description %]</td>
			</tr>
			</table>
		</div>
		<div class="sd-summary">
			<table class="summary-app">
				<caption>SHIPMENT INFORMATION </caption>
				<tr>
					<td class="text-align-right bold-text" style="width: 50%"> Carrier : </td>
					<td>[% shipmentinfo.carrier %]</td>
				</tr>
				<tr>
					<td class="text-align-right bold-text" style="width: 50%"> Service : </td>
					<td>[% shipmentinfo.service %]</td>
				</tr>
				<tr>
					<td class="text-align-right bold-text" style="width: 50%"> Order Number : </td>
					<td>[% coinfo.ordernumber %]</td>
				</tr>
				<tr>
					<td class="text-align-right bold-text" style="width: 50%"> Tracking Number : </td>
					<td>[% shipmentinfo.tracking1 %]</td>
				</tr>
				<tr>
					<td class="text-align-right bold-text" style="width: 50%">Ship Date : </td>
					<td>[% shipdate %]</td>
				</tr>
				<tr>
					<td class="text-align-right bold-text" style="width: 50%">Due Date : </td>
					<td>[% duedate ? duedate : 'Not Define' %]</td>
				</tr>
				[% IF coinfo.description %]
				<tr>
					<td class="text-align-right bold-text" style="width: 50%">Comments : </td>
					<td>[% coinfo.description %]</td>
				</tr>
				[% END %]
				[% IF coinfo.ponumber %]
				<tr>
					<td class="text-align-right bold-text" style="width: 50%">PO Number : </td>
					<td>[% coinfo.ponumber %]</td>
				</tr>
				[% END %]
			</table>
		</div>
	</div>
</div>

<div>
<h2>Package Detail</h2>
	<ul class="sortable css3-checkboxes" id="package-detail-list">
		<li class="ui-state-fixed">
			<table style="font-size: 11px">
				<tr>
					<th style="width: 04%">QTY</th>
					<th style="width: 04%">SKU#</th>
					<th style="width: 11%">Description</th>
					<th style="width: 10%">Type</th>
					<th style="width: 07%">Weight</th>
					<th style="width: 07%">Dim Weight</th>
					<th style="width: 05%">Length</th>
					<th style="width: 05%">Width</th>
					<th style="width: 05%">Height</th>
					<th style="width: 07%">Density</th>
					<th style="width: 05%">NMFC</th>
					<th style="width: 05%">Class</th>
					<th style="width: 07%">Dec. Value</th>
					<th style="width: 07%">Freight Ins.</th>
				</tr>
				<tr>
				<td colspan="3">&nbsp;</td>
				</tr>
			</table>
		</li>
		[% FOREACH  PackProData IN packagedetails %]
		<li class="ui-state-package ui-state-fixed">
			<table>
				<tr>
					<td style="width: 04%">
						<input  type="text" class="total_pkg_detail float-right labellike" name="quantity_1" id="quantity_1" value="[% PackProData.quantity %]" readonly/>
					</td>
					<td style="width: 04%">
						<input  type="text" class="sku_id  labellike" name="sku_1" id="sku_1" value = "[% PackProData.partnumber %]" readonly/>
					</td>
					<td style="width: 11%">
						<input  type="text" class="labellike" name="description_1" id="description_1"  value = "[% PackProData.description %]" readonly/>
					</td>
					<td style="width: 10%">
						<input  type="text" class="labellike" name="unittype_1" id="unittype_1"  value = "[% PackProData.unittype.unittypename %]" readonly/>
					</td>
					<td style="width: 07%">
						<input  type="text" class="total_pkg_detail labellike" name="weight_1" id="weight_1"  value = "[% PackProData.weight%]" readonly/>
					</td>
					<td style="width: 07%">
						<input  type="text" class="total_pkg_detail labellike" name="dimweight_1" id="dimweight_1"  value = "[% PackProData.dimweight%]" readonly/>
					</td>
					<td style="width: 05%">
						<input  type="text" class="total_pkg_detail labellike" name="dimlength_1" id="dimlength_1"  value = "[% PackProData.dimlength%]" readonly/>
					</td>
					<td style="width: 05%">
						<input  type="text" class="total_pkg_detail labellike" name="dimwidth_1" id="dimwidth_1"  value = "[% PackProData.dimwidth%]" readonly/>
					</td>
					<td style="width: 05%">
						<input  type="text" class="total_pkg_detail labellike" name="dimheight_1" id="dimheight_1"  value = "[% PackProData.dimheight%]" readonly/>
					</td>
					<td style="width: 07%">
						<input  type="text" class="labellike" name="density_1" id="density_1"  value = "[% PackProData.density%]" readonly/>
					</td>
					<td style="width: 05%">
						<input  type="text" class="labellike" name="nmfc_1" id="nmfc_1"  value = "[% PackProData.nmfc%]" readonly/>
					</td>
					<td style="width: 05%">
						<input  type="text" class="labellike" name="class_1" id="class_1"  value = "[% PackProData.class%]" readonly/>
					</td>
					<td style="width: 07%">
						<input  type="text" class="total_pkg_detail labellike" name="decval_1" id="decval_1" value="0.00"  value = "[% PackProData.decval%]" readonly/>
					</td>
					<td style="width: 07%">
						<input  type="text" class="total_pkg_detail labellike"  name="frtins_1" id="frtins_1"  value="[% PackProData.frtins%]" readonly/>
					</td>
				</tr>
			</table>
		</li>
		[% END %]
	</ul>
</div>